  },
  {
    "Key": "rootpass",
    "DisplayName": "root密码",
    "Value": "${parameters.MYSQLROOTPASSWD}"
  },
  {
    "Key": "backupkeep",
    "DisplayName": "备份保留周期",
    "Value": "${parameters.MYSQLBACKUPKEEP}"
  }
