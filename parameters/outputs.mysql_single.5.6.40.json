    "Value": "${outputs.mysql_instance.instanceCode}(${outputs.mysql_instance.privateIp})"
  },
  {
    "Key": "mysqlpath",
    "DisplayName": "mysql数据路径路径",
    "Value": "${parameters.mount_data_volume.MOUNTPOINT}"
  },
  {
    "Key": "mysqlbackuppath",
    "DisplayName": "mysql备份路径",
